<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd
	">

	<util:list id="converters">
		<!--
			Add your own converters using entries of the following form:
			<bean class="converters.Class1ToClass2" />
		-->

		<bean class="converters.StringToBox" />
		<bean class="converters.StringToMessage" />
		<bean class="converters.BoxToString" />
		<bean class="converters.MessageToString" />
		<bean class="converters.StringToWarranty" />
		<bean class="converters.WarrantyToString" />
		<bean class="converters.StringToCategory" />
		<bean class="converters.CategoryToString" />
		<bean class="converters.StringToCustomer" />
		<bean class="converters.CustomerToString" />
		<bean class="converters.StringToCreditCard" />
		<bean class="converters.CreditCardToString" />
		<bean class="converters.StringToFixUpTask" />
		<bean class="converters.FixUpTaskToString" />
		<bean class="converters.ActorToString" />
		<bean class="converters.StringToActor" />
		<bean class="converters.FinderToString" />
		<bean class="converters.HandyWorkerToString" />
		<bean class="converters.StringToActor" />
		<bean class="converters.StringToFinder" />
		<bean class="converters.StringToHandyWorker" />	
		<bean class="converters.StringToComplaint" />
		<bean class="converters.ComplaintToString" />
		<bean class="converters.StringToReport" />
		<bean class="converters.ReportToString" />	
			

	</util:list>

</beans>
